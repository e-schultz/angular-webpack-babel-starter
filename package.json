{
  "name": "angular-webpack-babel-starter",
  "version": "1.0.0",
  "description": "Skeleton project for angular + webpack + babel/ES6, test coverage, etc",
  "repository": "https://github.com/e-schultz/angular-webpack-babel-starter/",
  "main": "src/app.js",
  "scripts": {
    "build": "webpack --config webpack.build.js --bail -p",
    "dev": "node node_modules/.bin/webpack-dev-server --history-api-fallback --hot --inline --progress",
    "test": "node node_modules/eslint/bin/eslint .; node node_modules/karma/bin/karma start && node_modules/istanbul/lib/cli.js check-coverage --lines=70",
    "test:live": "node node_modules/karma/bin/karma start --auto-watch --no-single-run",
    "postinstall": "npm run build",
    "server": "npm run build; node server.js"
  },
  "author": "e-schultz",
  "license": "MIT",
  "dependencies": {
    "angular": "^1.4.3  ",
    "angular-translate": "^2.7.2",
    "angular-ui-router": "^0.2.15",
    "autoprefixer-core": "^5.2.0",
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "^5.5.8",
    "babel-loader": "^5.1.4",
    "babel-runtime": "^5.5.8",
    "bootstrap": "^3.3.5",
    "css-loader": "^0.14.5",
    "eslint-loader": "^1.0.0",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "html-webpack-plugin": "^1.5.2",
    "ng-annotate-loader": "0.0.6",
    "node-libs-browser": "^0.5.2",
    "node-sass": "^3.2.0",
    "normalize.css": "^3.0.3",
    "raw-loader": "^0.5.1",
    "sass-loader": "^2.0.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.1",
    "chai": "1.10.0",
    "eslint": "^1.1.0",
    "isparta-instrumenter-loader": "^0.2.1",
    "istanbul": "^0.3.17",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "0.2.4",
    "karma-coverage": "^0.4.2",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.0",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.5",
    "karma-spec-reporter": "0.0.19",
    "karma-webpack": "1.7.0",
    "mocha": "^2.2.5",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.17",
    "sinon": "^1.15.4"
  }
}
